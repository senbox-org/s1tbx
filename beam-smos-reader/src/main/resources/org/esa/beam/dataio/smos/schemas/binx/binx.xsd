<?xml version="1.0" encoding="UTF-8"?>
<!-- XML schema for binx 1.2 -->
<!-- binx.xsd, v 1.1 20/01/2005 -->
<!-- edikt::BinX -->
<!-- www.edikt.org -->
<!-- support@edikt.org -->
